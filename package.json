    "bin-editor-next": "^1.1.0",
    "element-plus": "^2.4.3",
    "jsplumb": "^2.15.6",
    "mitt": "^3.0.0",
    "mttk-vue-wrap": "^0.2.5",
    "nprogress": "^0.2.0",
    "sortablejs": "^1.15.0",
    "source-map-explorer": "^2.5.3",
    "splitpanes": "^3.1.5",
    "vue": "^3.4.21",
    "vue-draggable-plus": "^0.3.5",
    "vue-i18n": "^9.11.0",
    "vue-router": "^4.1.6"
